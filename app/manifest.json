{
  "name": "Bookmarker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "An intelligent way of organising bookmarks",
  "icons": {
    "16": "images/icon-16.png",
    "39": "images/icon-39.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "images/icon-39.png",
      "default_popup": "index.html"
  },
  "permissions": [ "activeTab",
    "storage",          "history"]
}
